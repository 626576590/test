<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration >
  
  <!-- 指定数据连接驱动jar地址 -->
  <classPathEntry location="./lib/mysql-connector-java-5.1.26-bin.jar" />
  
  <!--
  	targetRuntime:
  		MyBatis3:默认值;MyBatis3Simple 不生成和Example查询有关的内容,避免在后面的<table>中逐个进行配置
  	defaultModelType:
  		conditional:默认值,如果表有联合主键,那么该模型会产生一个单独的主键实体类; flat:该模型为每一张表只生成一个实体类.
  
   -->
  <context id="context1" targetRuntime="MyBatis3" defaultModelType="flat">
    <!-- 注释 -->  
    <commentGenerator >  
        <property name="suppressAllComments" value="true"/><!-- 是否取消注释 -->  
        <property name="suppressDate" value="true" /> <!-- 是否生成注释带时间戳-->  
    </commentGenerator>
    
    <!-- jdbc连接 --> 
   <!--  <jdbcConnection driverClass="com.mysql.jdbc.Driver" 
        connectionURL="jdbc:Mysql://localhost:3307/test?characterEncoding=utf8" 
        userId="root" 
        password="" /> -->
    <!-- jdbc连接 --> 
    <jdbcConnection driverClass="com.mysql.jdbc.Driver" 
        connectionURL="jdbc:Mysql://120.24.152.225:3306/call?characterEncoding=utf8" 
        userId="root" 
        password="mecmysql1401wTd" /> 
    
    <!-- 类型转换 --> 
    <javaTypeResolver>
        <!-- 是否使用bigDecimal， false可自动转化以下类型（Long, Integer, Short, etc.） --> 
        <property name="forceBigDecimals" value="true"/>
    </javaTypeResolver>
    
    <javaModelGenerator targetPackage="com.cecsm.core.entity" targetProject="mpsp">
        <!-- 是否继承指定类 -->
         <property name="rootClass" value="com.cecsm.common.bean.Base"/> 
        <!-- 是否在当前路径下新加一层schema,eg：fase路径com.els.common.vo， true:com.batian.modules.common.vo.[schemaName] -->
        <property name="enableSubPackages" value="true" />
        <!-- 是否针对string类型的字段在set的时候进行trim调用 --> 
        <property name="trimStrings" value="true"/>
    </javaModelGenerator>
    
    <!-- 生成*Mapper.xml文件 -->
    <sqlMapGenerator targetPackage="mybatis" targetProject="mpsp">
        <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>
    
    <!-- 生成对应接口DAO及DaoImpl -->
    <javaClientGenerator targetPackage="com.cecsm.core.mapper" targetProject="mpsp" type="XMLMAPPER">
        <property name="enableSubPackages" value="true" />
    </javaClientGenerator>
    
    <!-- Table配置 -->
    <table tableName="user" domainObjectName="User" 
    enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"
    enableSelectByExample="false" selectByExampleQueryId="false">
    	<property name="useActualColumnNames" value="true"/>   
    </table>
   <!--  <table tableName="apply_info_file" domainObjectName="ApplyInfoFile" 
    enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"
    enableSelectByExample="false" selectByExampleQueryId="false">
    	<property name="useActualColumnNames" value="true"/>  
    </table> -->
	
 
  
    
  </context>
</generatorConfiguration>